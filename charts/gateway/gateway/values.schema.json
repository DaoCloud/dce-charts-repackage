{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "gateway": {
            "type": "object",
            "properties": {
                "affinity": {
                    "type": "object"
                },
                "annotations": {
                    "type": "object"
                },
                "autoscaling": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "maxReplicas": {
                            "type": "integer"
                        },
                        "minReplicas": {
                            "type": "integer"
                        },
                        "targetCPUUtilizationPercentage": {
                            "type": "integer"
                        }
                    }
                },
                "containerSecurityContext": {
                    "type": "null"
                },
                "env": {
                    "type": "object"
                },
                "imagePullPolicy": {
                    "type": "string"
                },
                "imagePullSecrets": {
                    "type": "array"
                },
                "kind": {
                    "type": "string"
                },
                "labels": {
                    "type": "object"
                },
                "name": {
                    "type": "string"
                },
                "networkGateway": {
                    "type": "string"
                },
                "nodeSelector": {
                    "type": "object"
                },
                "podAnnotations": {
                    "type": "object",
                    "properties": {
                        "inject.istio.io/templates": {
                            "type": "string"
                        },
                        "prometheus.io/path": {
                            "type": "string"
                        },
                        "prometheus.io/port": {
                            "type": "string"
                        },
                        "prometheus.io/scrape": {
                            "type": "string"
                        },
                        "sidecar.istio.io/inject": {
                            "type": "string"
                        }
                    }
                },
                "rbac": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "replicaCount": {
                    "type": "integer"
                },
                "resources": {
                    "type": "object",
                    "properties": {
                        "limits": {
                            "type": "object",
                            "properties": {
                                "cpu": {
                                    "type": "string"
                                },
                                "memory": {
                                    "type": "string"
                                }
                            }
                        },
                        "requests": {
                            "type": "object",
                            "properties": {
                                "cpu": {
                                    "type": "string"
                                },
                                "memory": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "revision": {
                    "type": "string"
                },
                "securityContext": {
                    "type": "null"
                },
                "service": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object"
                        },
                        "externalIPs": {
                            "type": "array"
                        },
                        "externalTrafficPolicy": {
                            "type": "string"
                        },
                        "loadBalancerIP": {
                            "type": "string"
                        },
                        "loadBalancerSourceRanges": {
                            "type": "array"
                        },
                        "ports": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "port": {
                                        "type": "integer"
                                    },
                                    "protocol": {
                                        "type": "string"
                                    },
                                    "targetPort": {
                                        "type": "integer"
                                    }
                                }
                            }
                        },
                        "type": {
                            "type": "string"
                        }
                    }
                },
                "serviceAccount": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object"
                        },
                        "create": {
                            "type": "boolean"
                        },
                        "name": {
                            "type": "string"
                        }
                    }
                },
                "tolerations": {
                    "type": "array"
                },
                "topologySpreadConstraints": {
                    "type": "array"
                }
            }
        }
    }
}
