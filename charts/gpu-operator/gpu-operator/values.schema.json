{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "gpu-operator": {
            "type": "object",
            "properties": {
                "operator": {
                    "type": "object",
                    "properties": {
                        "initContainer": {
                            "type": "object",
                            "properties": {
                                "image": {
                                    "type": "string"
                                },
                                "repository": {
                                    "type": "string"
                                },
                                "version": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "driver": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "image": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "version": {
                            "type": "string"
                        },
                        "repoConfig": {
                            "type": "object",
                            "properties": {
                                "configMapName": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "toolkit": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "image": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "version": {
                            "type": "string"
                        }
                    }
                },
                "migManager": {
                    "type": "object",
                    "properties": {
                        "config": {
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
