{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "docker-registry": {
            "type": "object",
            "properties": {
                "affinity": {
                    "type": "object"
                },
                "autoscaling": {
                    "type": "object",
                    "properties": {
                        "behavior": {
                            "type": "object"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "maxReplicas": {
                            "type": "integer"
                        },
                        "minReplicas": {
                            "type": "integer"
                        },
                        "targetCPUUtilizationPercentage": {
                            "type": "integer"
                        },
                        "targetMemoryUtilizationPercentage": {
                            "type": "integer"
                        }
                    }
                },
                "configData": {
                    "type": "object",
                    "properties": {
                        "health": {
                            "type": "object",
                            "properties": {
                                "storagedriver": {
                                    "type": "object",
                                    "properties": {
                                        "enabled": {
                                            "type": "boolean"
                                        },
                                        "interval": {
                                            "type": "string"
                                        },
                                        "threshold": {
                                            "type": "integer"
                                        }
                                    }
                                }
                            }
                        },
                        "http": {
                            "type": "object",
                            "properties": {
                                "addr": {
                                    "type": "string"
                                },
                                "debug": {
                                    "type": "object",
                                    "properties": {
                                        "addr": {
                                            "type": "string"
                                        },
                                        "prometheus": {
                                            "type": "object",
                                            "properties": {
                                                "enabled": {
                                                    "type": "boolean"
                                                },
                                                "path": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                },
                                "headers": {
                                    "type": "object",
                                    "properties": {
                                        "X-Content-Type-Options": {
                                            "type": "array",
                                            "items": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        },
                        "log": {
                            "type": "object",
                            "properties": {
                                "fields": {
                                    "type": "object",
                                    "properties": {
                                        "service": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        },
                        "storage": {
                            "type": "object",
                            "properties": {
                                "cache": {
                                    "type": "object",
                                    "properties": {
                                        "blobdescriptor": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        },
                        "version": {
                            "type": "number"
                        }
                    }
                },
                "containerSecurityContext": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "deployment": {
                    "type": "object"
                },
                "extraEnvVars": {
                    "type": "array"
                },
                "extraVolumeMounts": {
                    "type": "array"
                },
                "extraVolumes": {
                    "type": "array"
                },
                "garbageCollect": {
                    "type": "object",
                    "properties": {
                        "deleteUntagged": {
                            "type": "boolean"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "schedule": {
                            "type": "string"
                        }
                    }
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "pullPolicy": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "ingress": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object"
                        },
                        "className": {
                            "type": "string"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "hosts": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "labels": {
                            "type": "object"
                        },
                        "path": {
                            "type": "string"
                        },
                        "tls": {
                            "type": "null"
                        }
                    }
                },
                "initContainers": {
                    "type": "array"
                },
                "metrics": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "port": {
                            "type": "integer"
                        },
                        "prometheusRule": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                },
                                "labels": {
                                    "type": "object"
                                },
                                "rules": {
                                    "type": "object"
                                }
                            }
                        },
                        "serviceMonitor": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                },
                                "labels": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "nodeSelector": {
                    "type": "object"
                },
                "persistence": {
                    "type": "object",
                    "properties": {
                        "accessMode": {
                            "type": "string"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "size": {
                            "type": "string"
                        }
                    }
                },
                "podAnnotations": {
                    "type": "object"
                },
                "podDisruptionBudget": {
                    "type": "object"
                },
                "podLabels": {
                    "type": "object"
                },
                "priorityClassName": {
                    "type": "string"
                },
                "proxy": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "password": {
                            "type": "string"
                        },
                        "remoteurl": {
                            "type": "string"
                        },
                        "secretRef": {
                            "type": "string"
                        },
                        "username": {
                            "type": "string"
                        }
                    }
                },
                "replicaCount": {
                    "type": "integer"
                },
                "resources": {
                    "type": "object"
                },
                "secrets": {
                    "type": "object",
                    "properties": {
                        "haSharedSecret": {
                            "type": "string"
                        },
                        "htpasswd": {
                            "type": "string"
                        }
                    }
                },
                "securityContext": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "fsGroup": {
                            "type": "integer"
                        },
                        "runAsUser": {
                            "type": "integer"
                        }
                    }
                },
                "service": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object"
                        },
                        "name": {
                            "type": "string"
                        },
                        "port": {
                            "type": "integer"
                        },
                        "type": {
                            "type": "string"
                        }
                    }
                },
                "serviceAccount": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object"
                        },
                        "create": {
                            "type": "boolean"
                        },
                        "name": {
                            "type": "string"
                        }
                    }
                },
                "storage": {
                    "type": "string"
                },
                "tolerations": {
                    "type": "array"
                },
                "updateStrategy": {
                    "type": "object"
                }
            }
        }
    }
}
