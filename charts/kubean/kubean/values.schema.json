{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "kubean": {
            "type": "object",
            "properties": {
                "affinity": {
                    "type": "object"
                },
                "fullnameOverride": {
                    "type": "string"
                },
                "global": {
                    "type": "object",
                    "properties": {
                        "imagePullSecrets": {
                            "type": "array"
                        },
                        "imageRegistry": {
                            "type": "string"
                        }
                    }
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "pullPolicy": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "imagePullSecrets": {
                    "type": "array"
                },
                "installCRDs": {
                    "type": "boolean"
                },
                "nameOverride": {
                    "type": "string"
                },
                "nodeSelector": {
                    "type": "object"
                },
                "podAnnotations": {
                    "type": "object"
                },
                "podSecurityContext": {
                    "type": "object"
                },
                "replicaCount": {
                    "type": "integer"
                },
                "resources": {
                    "type": "object"
                },
                "securityContext": {
                    "type": "object"
                },
                "service": {
                    "type": "object",
                    "properties": {
                        "port": {
                            "type": "integer"
                        },
                        "type": {
                            "type": "string"
                        }
                    }
                },
                "serviceAccount": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object"
                        },
                        "create": {
                            "type": "boolean"
                        },
                        "name": {
                            "type": "string"
                        }
                    }
                },
                "tolerations": {
                    "type": "array"
                }
            }
        }
    }
}
