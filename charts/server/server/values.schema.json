{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "server": {
            "type": "object",
            "properties": {
                "affinity": {
                    "type": "object"
                },
                "appKind": {
                    "type": "string"
                },
                "appName": {
                    "type": "string"
                },
                "commonAnnotations": {
                    "type": "object"
                },
                "commonLabels": {
                    "type": "object"
                },
                "deploymentReplicas": {
                    "type": "integer"
                },
                "dns": {
                    "type": "object",
                    "properties": {
                        "appDnsTcpPort": {
                            "type": "integer"
                        },
                        "appDnsTcpTlsPort": {
                            "type": "integer"
                        },
                        "appDnsUdpPort": {
                            "type": "integer"
                        }
                    }
                },
                "extraEnv": {
                    "type": "array"
                },
                "grpcServer": {
                    "type": "object",
                    "properties": {
                        "port": {
                            "type": "integer"
                        }
                    }
                },
                "hostNetwork": {
                    "type": "boolean"
                },
                "http": {
                    "type": "object",
                    "properties": {
                        "appHttpPort": {
                            "type": "integer"
                        },
                        "appHttpsPort": {
                            "type": "integer"
                        },
                        "healthPort": {
                            "type": "integer"
                        },
                        "livenessProbe": {
                            "type": "object",
                            "properties": {
                                "failureThreshold": {
                                    "type": "integer"
                                },
                                "periodSeconds": {
                                    "type": "integer"
                                }
                            }
                        },
                        "readinessProbe": {
                            "type": "object",
                            "properties": {
                                "failureThreshold": {
                                    "type": "integer"
                                },
                                "periodSeconds": {
                                    "type": "integer"
                                }
                            }
                        },
                        "startupProbe": {
                            "type": "object",
                            "properties": {
                                "failureThreshold": {
                                    "type": "integer"
                                },
                                "periodSeconds": {
                                    "type": "integer"
                                }
                            }
                        },
                        "tlsIgnoreClientCert": {
                            "type": "boolean"
                        }
                    }
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "digest": {
                            "type": "string"
                        },
                        "imagePullSecrets": {
                            "type": "array"
                        },
                        "pullPolicy": {
                            "type": "string"
                        },
                        "registry": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "ingress": {
                    "type": "object",
                    "properties": {
                        "enable": {
                            "type": "boolean"
                        },
                        "host": {
                            "type": "string"
                        },
                        "ingressClass": {
                            "type": "null"
                        },
                        "targetPath": {
                            "type": "string"
                        }
                    }
                },
                "nodeSelector": {
                    "type": "object",
                    "properties": {
                        "kubernetes.io/os": {
                            "type": "string"
                        }
                    }
                },
                "podAnnotations": {
                    "type": "object"
                },
                "podLabels": {
                    "type": "object"
                },
                "resources": {
                    "type": "object"
                },
                "securityContext": {
                    "type": "object"
                },
                "service": {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string"
                        }
                    }
                },
                "tls": {
                    "type": "object",
                    "properties": {
                        "client": {
                            "type": "object",
                            "properties": {
                                "secretName": {
                                    "type": "string"
                                }
                            }
                        },
                        "method": {
                            "type": "string"
                        },
                        "provided": {
                            "type": "object",
                            "properties": {
                                "tlsCert": {
                                    "type": "string"
                                },
                                "tlsKey": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "tolerations": {
                    "type": "object"
                },
                "updateStrategy": {
                    "type": "object",
                    "properties": {
                        "rollingUpdate": {
                            "type": "object",
                            "properties": {
                                "maxUnavailable": {
                                    "type": "integer"
                                }
                            }
                        },
                        "type": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}
